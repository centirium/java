package org.cuatrovientos.bucles;

/**
 * @author javier_lorda
 *
 */
public class Bucles {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// Bucles infinito everywhere

		int a = 10;
		while (a > 0) {
			System.out.println("voy todo buclao" + a);
			a--;
		}

		do {
			a++;
	}while (a<10);
	
		for (i=0;i<10;i++){
		System.out.println(i);
		}
	
	
	}

}
