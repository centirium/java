public class Operadores {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int a=0;
		int b;
		int c;
		// aritmeticas
		// +.-.*./.%
		
		c=a+b;
		c=a%b;
		c=-a;
		//comparacion
		
		
		//booleanas

	}

}
